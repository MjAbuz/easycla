[
  {
    "essential": true,
    "memory": 256,
    "image": "433610389961.dkr.ecr.us-west-2.amazonaws.com/consul:latest",
    "name": "consul-server",
    "portMappings": [
      {
        "containerPort": 8300,
        "hostPort": 8300,
        "protocol": "tcp"
      },
      {
        "containerPort": 8301,
        "hostPort": 8301,
        "protocol": "tcp"
      },
      {
        "containerPort": 8301,
        "hostPort": 8301,
        "protocol": "udp"
      },
      {
        "containerPort": 8302,
        "hostPort": 8302,
        "protocol": "tcp"
      },
      {
        "containerPort": 8400,
        "hostPort": 8400,
        "protocol": "tcp"
      },
      {
        "containerPort": 8500,
        "hostPort": 8500,
        "protocol": "tcp"
      },
      {
        "containerPort": 8600,
        "hostPort": 8600,
        "protocol": "udp"
      }
    ],
    "environment": [
      {
        "name": "CONSUL_DATACENTER",
        "value": "${CONSUL_DATACENTER}"
      },
      {
        "name": "CONSUL_ENCRYPTION_KEY",
        "value": "${CONSUL_ENCRYPTION_KEY}"
      },
      {
        "name": "EC2_REGION",
        "value": "${EC2_REGION}"
      },
      {
        "name": "EC2_TAG_KEY",
        "value": "${EC2_TAG_KEY}"
      },
      {
        "name": "EC2_TAG_VALUE",
        "value": "${EC2_TAG_VALUE}"
      },
      {
        "name": "SERVICE_IGNORE",
        "value": "true"
      }
    ],
    "logConfiguration": {
      "logDriver": "gelf",
      "options": {
        "gelf-address": "udp://logstash.service.consul:12201",
        "labels": "app_name,app_env,app_region"
      }
    },
    "dockerLabels": {
      "app_name": "consul",
      "app_env": "production",
      "app_region": "${EC2_REGION}"
    }
  }
]