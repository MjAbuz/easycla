[
  {
    "essential": true,
    "image": "433610389961.dkr.ecr.us-west-2.amazonaws.com/pypicloud:latest",
    "memoryReservation": 258,
    "name": "pypi",
    "portMappings": [{
      "containerPort": 8080,
      "hostPort": 8080
    }],
    "dnsServers": [
      "${DNS_SERVER_1}",
      "${DNS_SERVER_2}",
      "${DNS_SERVER_3}"
    ],
    "environment": [
      {
        "name": "AWS_ACCESS_KEY_ID",
        "value": "${AWS_ACCESS_KEY_ID}"
      },
      {
        "name": "AWS_SECRET_ACCESS_KEY",
        "value": "${AWS_SECRET_ACCESS_KEY}"
      },
      {
        "name": "S3_BUCKET",
        "value": "${S3_BUCKET}"
      },
      {
        "name": "REDIS_HOST",
        "value": "${REDIS_HOST}"
      },
      {
        "name": "SERVICE_NAME",
        "value": "pypi"
      },
      {
        "name": "SERVICE_TAGS",
        "value": "${TAG_REGION},${TAG_VPC_ID}"
      },
      {
        "name": "SERVICE_8080_CHECK_HTTP",
        "value": "/"
      },
      {
        "name": "SERVICE_8080_CHECK_INTERVAL",
        "value": "300s"
      }
    ],
    "logConfiguration": {
      "logDriver": "gelf",
      "options": {
        "gelf-address": "udp://${TAG_REGION}.logstash.service.consul:12201",
        "labels": "app_name,app_env,app_region"
      }
    },
    "dockerLabels": {
      "app_name": "pypi",
      "app_env": "production",
      "app_region": "${TAG_REGION}"
    }
  }
]