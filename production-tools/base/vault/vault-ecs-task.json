[
  {
    "essential": true,
    "memory": 256,
    "image": "433610389961.dkr.ecr.us-west-2.amazonaws.com/vault:latest",
    "name": "vault-server",
    "privileged": true,
    "portMappings": [
      {
        "containerPort": 8200,
        "hostPort": 8200,
        "protocol": "tcp"
      },
      {
        "containerPort": 8201,
        "hostPort": 8201,
        "protocol": "tcp"
      }
    ],
    "dnsServers": [
      "${DNS_SERVER_1}",
      "${DNS_SERVER_2}",
      "${DNS_SERVER_3}"
    ],
    "environment": [
      {
        "name": "CONSUL_MASTER_IP",
        "value": "${CONSUL_MASTER_IP}"
      },
      {
        "name": "SERVICE_IGNORE",
        "value": "true"
      }
    ],
    "logConfiguration": {
      "logDriver": "gelf",
      "options": {
        "gelf-address": "udp://${AWS_REGION}.logstash.service.consul:12201",
        "labels": "app_name,app_env,app_region"
      }
    },
    "dockerLabels": {
      "app_name": "vault",
      "app_env": "production",
      "app_region": "${AWS_REGION}"
    }
  }
]