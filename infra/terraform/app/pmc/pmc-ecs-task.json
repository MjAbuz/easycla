[
  {
    "essential": true,
    "image": "643009352547.dkr.ecr.us-west-2.amazonaws.com/ccc/production:${build_hash}",
    "memoryReservation": 512,
    "name": "ccc",
    "portMappings": [{
      "containerPort": 80
    }],
    "dnsServers": [
      "${DNS_SERVER_1}",
      "${DNS_SERVER_2}",
      "${DNS_SERVER_3}"
    ],
    "ulimits": [
      {
        "softLimit": 5000,
        "hardLimit": 5000,
        "name": "nofile"
      }
    ],
    "environment": [
      {
        "name": "APP_DOMAINS",
        "value": "${APP_DOMAINS}"
      },
      {
        "name": "SERVICE_8081_NAME",
        "value": "CCC_${build_hash}"
      },
      {
        "name": "SERVICE_8081_TAGS",
        "value": "${TAG_REGION},${TAG_VPC_ID}"
      },
      {
        "name": "NEWRELIC_LICENSE",
        "value": "true"
      }
    ],
    "logConfiguration": {
      "logDriver": "gelf",
      "options": {
        "gelf-address": "udp://${TAG_REGION}.logstash.service.consul:12201",
        "labels": "realm,application,version,environment,region,tags"
      }
    },
    "dockerLabels": {
      "realm": "ccc",
      "application": "ccc",
      "version": "${build_hash}",
      "environment": "production",
      "region": "${TAG_REGION}",
      "tags": "application, ccc, nodejs"
    }
  }
]
