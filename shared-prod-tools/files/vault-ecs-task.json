[
  {
    "essential": true,
    "memory": 256,
    "image": "433610389961.dkr.ecr.us-west-2.amazonaws.com/vault:latest",
    "name": "vault-server",
    "privileged": true,
    "portMappings": [
      {
        "containerPort": 8200,
        "hostPort": 8200,
        "protocol": "tcp"
      },
      {
        "containerPort": 8201,
        "hostPort": 8201,
        "protocol": "tcp"
      }
    ],
    "dnsServers": [
      "${VPC_DNS_SERVER}",
      "8.8.8.8"
    ],
    "environment": [
      {
        "name": "CONSUL_MASTER_IP",
        "value": "${CONSUL_MASTER_IP}"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "shared-production-infra",
        "awslogs-region": "us-west-2",
        "awslogs-stream-prefix": "vault"
      }
    }
  }
]