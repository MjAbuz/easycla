[
  {
    "essential": true,
    "image": "433610389961.dkr.ecr.us-west-2.amazonaws.com/dns_lb_nginx:latest",
    "memoryReservation": 256,
    "name": "nginx",
    "portMappings": [
      {
        "containerPort": 53,
        "hostPort": 53
      }
    ],
    "environment": [
      {
        "name": "SERVICE_NAME",
        "value": "nginx-dns-lb"
      },
      {
        "name": "SERVICE_TAGS",
        "value": "${TAG_REGION},${TAG_VPC_ID}"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "shared-production-infra",
        "awslogs-region": "us-west-2",
        "awslogs-stream-prefix": "dns_lb_nginx"
      }
    }
  }
]