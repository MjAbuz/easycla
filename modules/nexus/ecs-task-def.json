[
  {
    "essential": true,
    "image": "643009352547.dkr.ecr.us-west-2.amazonaws.com/infra/nexus:2.14.7-01",
    "memoryReservation": 1528,
    "name": "nexus",
    "portMappings": [{
      "containerPort": 8081,
      "hostPort": 8081
    }],
    "mountPoints": [
      {
        "sourceVolume": "nexus-storage",
        "containerPath": "/sonatype-work"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "infra",
        "awslogs-region": "us-west-2",
        "awslogs-stream-prefix": "nexus"
      }
    },
    "dockerLabels": {
      "app_name": "nexus",
      "app_env": "production",
      "app_region": "${TAG_REGION}"
    }
  }
]